{
  "name": "my-tic-tac-toe-app",
  "version": "1.0.0",
  "description": "Tic-tac-toe game two people can play on their phones and computers separatedly!",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
