  "license": "MIT",
  "dependencies": {
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "bootstrap": "^4.5.3",
    "framer-motion": "^2.9.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "3.4.3",
    "reactstrap": "^8.6.0"
  }
}
